# Uncomment the next line to define a global platform for your project
platform :ios, '11.0'

target 'ViewControllers' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  #9 Pods for ViewControllers
  pod 'Alamofire', '~> 4.4'
  pod 'RxSwift', '~> 4.4.0'
  pod 'RxCocoa', '~> 4.4.0'

  target 'ViewControllersTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'ViewControllersUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
